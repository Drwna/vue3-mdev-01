{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*.vue",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "types/**/*.d.ts",
    "types/**/*.ts",
    "src/**/*.tsx",
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": ["node"]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
